    "baseline-browser-mapping": "^2.9.19",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "husky": "^8.0.0",
    "lint-staged": "^16.2.7",
    "playwright": "^1.58.1",
    "tailwindcss": "^4",
    "typescript": "^5"
