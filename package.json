{
  "name": "elkaza-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "dev": "next dev -p 3002",
    "build": "next build",
    "start": "next start -p 3002",
    "lint": "eslint . --max-warnings=0",
    "typecheck": "tsc --noEmit",
